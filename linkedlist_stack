public class Node {
    private int item;
    private Node node;

    public Node(int item) {
        this.item = item;
        this.node = null;
    }

    protected void linkNode(Node node) {
        this.node = node;
    }
    protected int getData() {
        return this.item;
    }
    protected Node getNextNode() { 
        return this.node;
    }
}

public class NodeManager {
    Node top;

    public NodeManager() {
        this.top = null;
    }
    public void push(int data) {
        Node node = new Node(data); 
        node.linkNode(top);
        top = node;
    }
    public void pop() {
        top = top.getNextNode();
    }
    public int peek() {
        return top.getData();
    }
}
