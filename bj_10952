import java.util.Scanner;

public class Main{
    public static void main(String[] agrs){
        Scanner sc = new Scanner(System.in);
        
        while(true)
        {
            int A = sc.nextInt();
            int B = sc.nextInt();
            int X = A+B;
            if(X==0)
            {
                break;
            }
            System.out.println(A+B);
        }
    }
}
